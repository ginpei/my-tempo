{
  "name": "my-tempo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "firebase emulators:exec --import .firebase-export --export-on-exit --ui 'next dev'",
    "dev-real": "NEXT_PUBLIC_FIREBASE_USE_REAL=1 npx next dev",
    "firebase-clean": "rmdir firebase-export-*",
    "firebase-export": "firebase emulators:export .firebase-export",
    "format": "eslint --fix .",
    "lint": "next lint && npx tsc --project tsconfig.json",
    "start": "next start"
  },
  "dependencies": {
    "firebase": "^9.22.0",
    "next": "13.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "20.2.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "autoprefixer": "^10.4.14",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "firebase-tools": "^12.1.0",
    "git-cz": "^4.9.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4"
  }
}
